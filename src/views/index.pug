include base.pug
body
  .container
    h1(style='text-align: center')= message
    table(style='width:30rem;height:30rem;margin:auto;padding:10px;margin-bottom: 1rem')
      - for (var x = 0; x < 10; x++)
        tr(style='border:3px solid black')
          - for (var y = 0; y < 10; y++)
            td(style='border:3px solid black')
  .container
    form.form-inline(style='margin: 0 auto; width: 300px', action='', method='post')
      .form-group.mx-sm-3.mb-2
        input#coordinates.form-control(type='text', placeholder='Coordinates')
      button.btn.btn-primary.mb-2(type='submit') Submit
  script.
    websocket = new WebSocket('ws://localhost:8000');
    
    websocket.onopen = function(event) {
    websocket.send('Sending text to server!');
    };
    
    websocket.onmessage = function(event) {
    console.log(event.data);
    };
